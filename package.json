{
  "name": "gatsby-ts-sanity-starter",
  "version": "1.0.1",
  "description": "A starter for websites using Gastby 4 with Typescript and Sanity Headless CMS\"",
  "main": "index.js",
  "author": "Bryan Hickey <bjh@bjh.dev> (https://bjh.dev/)",
  "license": "MIT",
  "private": true,
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "build": "lerna run build --parallel",
    "build-web": "lerna bootstrap && (cd web && yarn build)",
    "build-studio": "lerna bootstrap && (cd studio && yarn build)",
    "start": "lerna run start --parallel",
    "format": "lerna run format",
    "graphql-deploy": "lerna run graphql-deploy",
    "lint": "lerna run lint",
    "postinstall": "lerna bootstrap",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@sanity/cli": "^2.26.0",
    "lerna": "^4.0.0"
  }
}
